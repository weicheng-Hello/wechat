view.nav {
  width: 100%;
  height: 100rpx;
  background-color: #f93553;
  display: flex;
  justify-content: center;
  align-items: center;
}
view.nav navigator.input {
  width: 95%;
  height: 85%;
  background-color: white;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 5rpx;
}
view.content {
  margin-top: 10rpx;
  width: 100%;
  height: 150rpx;
  display: flex;
}
view.content img {
  flex: 1;
}
.fl {
  margin-top: 20rpx;
}
.fl .apt_name {
  color: pink;
  margin-bottom: 10rpx;
}
.fl view {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
}
.fl view navigator.apt_nav {
  width: 100%;
  width: 30%;
  margin-left: 2.6%;
}
.fl view navigator.apt_nav image {
  width: 100%;
  height: 200rpx;
}
